<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="C:/Users/nithan/New folder/ecommerce.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser pragmas query" current="3"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="1521"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="0" parent="0"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="order_items" custom_title="0" dock_id="1" table="4,11:mainorder_items"/><dock_state state="000000ff00000000fd00000001000000020000000000000000fc0100000001fb000000160064006f0063006b00420072006f00770073006500310100000000ffffffff0000011e00ffffff000000000000000000000004000000040000000800000008fc00000000"/><default_encoding codec=""/><browse_table_settings/></tab_browse><tab_sql><sql name="JOIN-Aggregation">SELECT
    u.user_id,
    u.first_name || ' ' || u.last_name AS user_name,
    COUNT(DISTINCT o.order_id) AS number_of_orders,
    SUM(oi.line_total) AS total_amount_spent,
    MAX(o.order_date) AS last_order_date
FROM users u
LEFT JOIN orders o ON u.user_id = o.user_id
LEFT JOIN order_items oi ON o.order_id = oi.order_id
LEFT JOIN products p ON oi.product_id = p.product_id
LEFT JOIN payments pay ON o.order_id = pay.order_id
GROUP BY u.user_id, user_name
ORDER BY total_amount_spent DESC;
</sql><sql name="Top-Selling Products ">SELECT
    p.product_id,
    p.name,
    SUM(oi.line_total) AS total_revenue,
    COUNT(oi.order_item_id) AS items_sold
FROM products p
JOIN order_items oi ON p.product_id = oi.product_id
GROUP BY p.product_id, p.name
ORDER BY total_revenue DESC
LIMIT 10;
</sql><sql name="Average-order-value">SELECT 
    AVG(order_total) AS avg_order_value
FROM (
    SELECT 
        o.order_id,
        SUM(oi.line_total) AS order_total
    FROM orders o
    JOIN order_items oi ON o.order_id = oi.order_id
    GROUP BY o.order_id
);
</sql><sql name="CLV">SELECT
    u.user_id,
    u.first_name || ' ' || u.last_name AS user_name,
    SUM(oi.line_total) AS lifetime_value
FROM users u
JOIN orders o ON u.user_id = o.user_id
JOIN order_items oi ON o.order_id = oi.order_id
GROUP BY u.user_id
ORDER BY lifetime_value DESC;
</sql><sql name="payment method ">SELECT 
    payment_method,
    COUNT(*) AS total_payments,
    SUM(amount) AS total_amount
FROM payments
GROUP BY payment_method
ORDER BY total_amount DESC;
</sql><sql name="order status ">SELECT 
    status,
    COUNT(*) AS total_orders
FROM orders
GROUP BY status
ORDER BY total_orders DESC;
</sql><current_tab id="5"/></tab_sql></sqlb_project>
